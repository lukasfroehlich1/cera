  "bugs": {
    "url": "https://github.com/lukasfroehlich1/cera/issues"
  },
  "homepage": "https://github.com/lukasfroehlich1/cera",
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "cookie-parser": "^1.4.1",
    "csurf": "^1.8.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "forms": "^1.1.4",
    "googlemaps": "^1.4.0",
    "materialize-css": "^0.97.6",
    "mocha": "^2.5.3",
    "mysql": "^2.10.2",
    "pug": "^2.0.0-alpha6",
    "typescript": "^1.8.10"
