{
  "name": "cera",
  "version": "1.0.0",
  "description": "ride sharing application",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc --module commonjs && npm run lint",
    "test": "./node_modules/.bin/mocha --require ts-node/register dist/test/*.js",
    "lint": "tslint -c tslint.json app/**.ts",
    "clean": "rm -rf node_modules && npm install && rm -rf typings && typings install",
    "pretest": "typings install && npm run build",
    "prepublish": "npm install typings --global && npm install -g tslint && typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukasfroehlich1/cera.git"
  },
  "author": "",
